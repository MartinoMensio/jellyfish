venv:
    rm -rf .venv
    python3 -m venv .venv
    . .venv/bin/activate
    .venb/bin/pip install wheel 
    .venb/bin/pip install jupyter matplotlib pandas


timedruns:
    .venv/bin/pip install jellyfish==0.10.0 # last C version
    .venv/bin/python timedruns.py old > timedruns.csv
    .venv/bin/pip uninstall jellyfish
    .venv/bin/pip install -e ..
    .venv/bin/python timedruns.py new >> timedruns.csv